{
    "name": "grechanyuk/freekassa",
    "description": "FreeKassa payments for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Egor G.",
            "email": "xorolllo@hotmail.com"
        }
    ],
    "homepage": "https://github.com/grechanyuk/freekassa",
    "keywords": ["Laravel", "FreeKassa"],
    "require": {
        "illuminate/support": "~5",
        "guzzlehttp/guzzle": "^6.3",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Grechanyuk\\FreeKassa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Grechanyuk\\FreeKassa\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Grechanyuk\\FreeKassa\\FreeKassaServiceProvider"
            ],
            "aliases": {
                "FreeKassa": "Grechanyuk\\FreeKassa\\Facades\\FreeKassa"
            }
        }
    }
}
